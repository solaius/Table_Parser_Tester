# Environment variables for Table Parser Tester
# Copy this file to .env and fill in your values

# Granite Vision API endpoint
# Example: https://your-company-name.your-domain.com/api/v1/granite-vision/
GRANITE_VISION_ENDPOINT=https://your-granite-vision-endpoint.com/

# Replicate API token (if using Replicate instead of custom endpoint)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Optional: Set to 'true' to enable debug mode
DEBUG=false